    "# 訓練資料的預測\n",
    "yhat_train = model.predict(train_X)\n",
    "yhat_train[yhat_train>=0.5] = 1\n",
    "yhat_train[yhat_train<0.5] = 0\n",
    "print('-------------訓練集-------------')\n",
