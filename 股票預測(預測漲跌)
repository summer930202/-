    "stock_id = '2330.TW'\n",
    "data = yf.Ticker(stock_id)\n",
    "df_stock = data.history(period=\"max\")\n",
    "# 增加股票代號\n",
    "df_stock['stock_id'] = stock_id\n",
    "df_stock"
